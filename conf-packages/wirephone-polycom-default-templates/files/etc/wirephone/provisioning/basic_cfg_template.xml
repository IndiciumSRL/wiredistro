  <call call.callsPerLineKey="${call_callsPerLineKey}">
  </call>
      <reg
    % for device in devices:
        reg.${loop.index+1}.address="${device.name}" 
        reg.${loop.index+1}.displayName="${device.name}"
        reg.${loop.index+1}.auth.password="${device.params['password'].val}"
        reg.${loop.index+1}.auth.userId="${device.name}"
        reg.${loop.index+1}.label="${device.name}"
        reg.${loop.index+1}.outboundProxy.address="${ippbx}"
    % endfor
    >
    </reg>
<!-- sip-basic -->
