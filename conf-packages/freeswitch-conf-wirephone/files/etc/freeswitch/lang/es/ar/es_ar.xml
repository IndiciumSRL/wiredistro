<include>
  <language name="es_ar" say-module="es_ar" sound-prefix="$${sounds_dir}/es/ar/mario" tts-engine="cepstral" tts-voice="marta">
    <phrases>
      <macros>
        <X-PRE-PROCESS cmd="include" data="vm/sounds-es-ar.xml"/> <!-- vm/tts.xml if you want to use tts and have cepstral -->
        <X-PRE-PROCESS cmd="include" data="padlock/*.xml"/>  <!-- IVR and custom phrases go here -->
        <X-PRE-PROCESS cmd="include" data="auth_pin/*.xml"/>  <!-- IVR and custom phrases go here -->
      </macros>
    </phrases>
  </language>
</include>
